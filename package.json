{
  "name": "@faxi/web-component-library",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Faxi-Kinto/web-component-library"
  },
  "main": "build/index.js",
  "module": "build/index.es.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run copy-files && tsc -w",
    "build": "npm run copy-files && tsc",
    "copy-files": "copyfiles -u 1 \"src/**/*.css\" build",
    "prepare": "npm run build",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose --env=jsdom",
    "test:coverage": "jest --verbose --coverage",
    "test:ci": "cross-env CI=true jest --env=jsdom",
    "lint": "eslint --ext .tsx --ext .ts src/ --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -s ./public -p 9009",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:ci && npm run lint",
      "pre-push": "npm run test:ci && npm run lint"
    }
  },
  "peerDependencies": {},
  "devDependencies": {
    "@storybook/theming": "^5.3.19",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "@types/xregexp": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "copyfiles": "^2.3.0"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@faxi/web-css-utilities": "^1.0.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^2.1.2",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^7.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-watch": "^6.0.1",
    "html-react-parser": "^0.10.5",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.2",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.1.1",
    "ts-jest": "^25.5.1",
    "tslib": "^2.0.0",
    "typescript": "^3.7.5",
    "xregexp": "^4.3.0"
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "build/"
    ]
  }
}
